{
  "name": "obsidian-notes-chat",
  "version": "1.0.0",
  "description": "Chat with your notes using AI - supports multiple LLM providers with budget tracking",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "load-docs": "node scripts/load-obsidian-docs.js",
    "generate-vault": "node scripts/generate-test-vault.js",
    "package": "npm run build && npm run package:zip",
    "package:zip": "node scripts/package-plugin.js",
    "release": "npm run test:ci && npm run build && npm run package",
    "deploy": "npm run release && node scripts/deploy-plugin.js",
    "test": "jest --testPathPattern=unit",
    "test:all": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "test:ci": "jest --ci --coverage --watchAll=false --testPathPattern=unit",
    "test:agent": "node tests/simple-agent-test.js",
    "test:agent-suite": "node tests/simple-agent-test.js --test",
    "dev-install": "node scripts/dev-install.js",
    "dev-uninstall": "node scripts/dev-install.js uninstall"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "llm",
    "ai",
    "chat",
    "notes",
    "knowledge-graph",
    "hotkey",
    "budget-tracking",
    "conversation"
  ],
  "author": "StraitStreet",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "archiver": "^6.0.0",
    "babel-jest": "^30.0.5",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.9",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "obsidian": "latest",
    "ts-jest": "^29.4.1",
    "tslib": "2.4.0",
    "tsx": "^4.7.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@xenova/transformers": "^2.6.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0",
    "chroma-js": "^2.4.2",
    "ollama-ai-provider": "^1.2.0",
    "tiktoken": "^1.0.0",
    "token.js": "^0.4.5"
  }
}
